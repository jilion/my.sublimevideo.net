%a.sublime{ href: "" }
%video.zoom{ id: "playable_video_#{@video.id}", 'data-uid' => "#{@video.id}", 'data-name' => "#{@video.get('n')}", style: "display:none", width: "#{@video.width()}", height: "#{@video.height()}", poster: "#{@video.get('p')}", preload: "none" }
  - for source in @video.currentSources()
    - if source.q != 'base'
      %source{ src: "#{source.u}", 'data-quality' => "#{source.q}" }
    - else
      %source{ src: "#{source.u}" }