@import "css3";

* {
  margin:0;
  padding:0;
}

html {min-height:100%;}

body {
  font-family:"proxima-nova-1","proxima-nova-2", Helvetica, Arial, sans-serif;
  font-size:14px;
  min-height:100%;
  background:#ddd;
  background:-webkit-gradient(linear,left top,left bottom,from(#eee),to(#ccc));
  background:-moz-linear-gradient(top, #eee, #ccc);
  text-rendering:optimizeLegibility;
}

#header {
  padding:15px;
  position:relative;
  min-width:830px;
  background:#222;
  background:-webkit-gradient(linear,left top,left bottom,from(#444),to(#000));
  background:-moz-linear-gradient(top, #444, #000);
}

#header h1 {
  font-size:28px;
  font-weight:400;
  float:left;
}

#header h1 a {
  color:#fff;
}

#header h1 em {
  color:rgba(255,255,255,0.3);
  font-style:normal;
}

#header .popup_menu {
  float:left;
  position:relative;
  margin:0 0 0 20px;
  z-index:100;
}

#header .popup_menu h3 {
  padding:4px;
  font-size:18px;
  font-weight:600;
  border-radius:13px;
  position:relative;
  z-index:2;
  background:#950041;
  background:-webkit-gradient(linear,left top,left bottom,from(#950041),to(#730033));
  background:-moz-linear-gradient(top, #950041, #730033);
}

#header .popup_menu:hover h3 {
  background:#ddd;
  padding-bottom:0;
  border-top-left-radius:8px;
  border-top-right-radius:8px;
  border-bottom-left-radius:0;
  border-bottom-right-radius:0;
}

#header .popup_menu h3 a {
  color:#fff;
  padding:2px 8px;
  display:block;
  text-shadow:#000 0 -1px 0;
  border-radius:4px;
}
#header .popup_menu:hover h3 a {
  background:#950041;
  background:-webkit-gradient(linear,left top,left bottom,from(#950041),to(#730033));
  background:-moz-linear-gradient(top, #950041, #730033);
  color:#fff;
}

#header .popup_menu ul {
  width:160px;
  position:absolute;
  display:none;
  background:#333;
  list-style-type:none;
  padding:2px 0;
  border-top-right-radius:8px;
  border-bottom-left-radius:8px;
  border-bottom-right-radius:8px;
  background:#eee;
  background:-webkit-gradient(linear,left top,left bottom,from(#ddd),to(#a9a9a9));
  background:-moz-linear-gradient(top, #ddd, #a9a9a9);
  -webkit-box-shadow:rgba(0,0,0,0.8) 0 2px 8px;
}

#header .popup_menu:hover ul {display:block}

#header .popup_menu ul li {
  padding:2px 4px;
  font-size:18px;
  font-weight:800;
  text-shadow:rgba(255,255,255,0.5) 0 1px 0;
}

#header .popup_menu ul li a {
  display:block;
  padding:6px 8px;
  color:#111;
  border-radius:4px;
}

#header .popup_menu ul li a:hover, #header .popup_menu ul li.active a:hover {
  background:#333;
  color:#fff;
  text-shadow:#000 0 -1px 0;
  -webkit-font-smoothing:antialiased;
}

#header .popup_menu ul li.active a {
  color:#777;
  font-weight:normal;
}

ul.favorite_links {
  list-style-type:none;
  float:left;
  padding:8px 0 0 15px;
}

ul.favorite_links li {
  float:left;
  padding:0 10px 0 0;
}

ul.favorite_links li a {
  font-size:16px;
  padding:8px 12px 7px;
  color:#fff;
  text-shadow:#000 0 -1px 0;
  background:#444;
  background:-webkit-gradient(linear,left top,left bottom,from(#555),to(#333));
  background:-moz-linear-gradient(top, #555, #333);
  border-radius:13px;
}

ul.favorite_links li a:hover {
  color:#fff;
}

ul.favorite_links li.active a {
  background:#777;
  background:-webkit-gradient(linear,left top,left bottom,from(#666),to(#777));
  background:-moz-linear-gradient(top, #666, #777);
  color:#fff;
  text-shadow:rgba(0,0,0,0.4) 0 -1px 0;
  -webkit-box-shadow:inset #000 0 1px 4px, #000 0 0 2px;
}

ul.no_bullet {
  list-style-type:none;
}

#header .right {
  width:150px;
  position:absolute;
  top:10px;
  right:15px;
}

#header .right ul {
  list-style-type:none;
  padding:12px 0 0;
}

#header .right li {
  float:right;
  padding:0 0 0 12px;
}

#header .right li a {
  color:rgba(255,255,255,0.4);
  -webkit-transition:0.3s color;
}

#header .right li a:hover {
  color:#fff;
}

#main {
  padding:15px;
  min-width:1004px;
  margin:15px;
  position:relative; /* remy */
  background:#fff;
  border-radius:10px;
}

#main a.add {
  background:url(<%= asset_path 'admin/add_icon.png' %>) no-repeat;
  padding:2px 0 5px 28px;
  font-size:16px;
  font-weight:600;
}

#main h2 {
  font-size:36px;
  font-weight:600;
  padding:0 0 10px;
}
#main h2.revenue {
  font-size:34px;
  padding:0;
}
#main h3.revenue {
  font-size:24px;
  padding:0;
}

#main .left, #main .left td {text-align:left}

#main table {
  width:100%;
  border-collapse:collapse;
  margin:15px 0 0;
}

#main table tr.alt {
  background:#eee;
}

// Thibaud Color Style! ROCK ON!
#main table tr.deleted {
  background:#ee605e;
}

#main table th {
  padding:8px 0;
  white-space:nowrap;
  background:#ddd;
  background:-webkit-gradient(linear,left top,left bottom,from(#eee),to(#ccc));
  background:-moz-linear-gradient(top, #eee, #ccc);
}

#main table td {
  padding:15px 5px;
  text-align:center;
}

#main table tr.recurring td.name {
  background:url(<%= asset_path 'admin/recurring_icon.png' %>) no-repeat 8px 50%;
  padding-left:35px;
}

#main table th a.sort:after {content:" ▾";}
#main table th a.sort.up:after {content:" ▴";}

#main table th a.active {
  background:#111;
  background:-webkit-gradient(linear,left top,left bottom,from(#444),to(#555));
  background:-moz-linear-gradient(top, #444, #555);
  color:#fff;
  padding:3px 10px;
  border-radius:20px;
  -webkit-box-shadow:inset #000 0 1px 4px;
}

#main form label {
  display:block;
  font-size:13px;
  font-weight:600;
  margin:10px 0 0;
}

#main form input.text {
  width:400px;
  font-size:16px;
  padding:3px;
}

#main form textarea {
  width:400px;
  padding:3px;
  font-size:16px;
}

#main form .actions input[type=submit] {
  margin:20px 0 0;
}

#main form p.note {
  margin:15px 0 0;
}

#main form .checkbox_wrap input {
  float:left;
  margin:1px 6px 0 0;
}

#footer {
  text-align:center;
  padding:10px 0;
}

.spacer {clear:both;}

a {
  text-decoration:none;
  color:#ca0063;
}

/* Dirty style */

#content table.main td.delete a, #content table.main td.delete input {
  text-indent:-9999px;
  display:block;
  width:20px;
  height:22px;
  background:url(<%= asset_path 'admin/trash_btn.png' %>) no-repeat;
  margin:0 10px;
  border:none;
}

#content form.site input.add, form input.submit.small {
  width:97px;
  height:48px;
  background:url(<%= asset_path 'admin/add_btn.png' %>) no-repeat;
  border:none;
  font-family:"proxima-nova-1","proxima-nova-2", Helvetica, Arial, sans-serif;
  font-size:18px;
  font-weight:600;
  text-align:center;
  color:#4d5563;
  text-shadow:rgba(255,255,255,0.45) 0 1px 0;
  outline:0;
  position:relative;
  left:-5px;
}

form label.icon {
  text-indent:-9999px;
  display:block;
  position:absolute;
}

/* remy */
#main .dashboard_box {
  float:left;
  background:-webkit-gradient(linear,left top,left bottom,from(#888),to(#BBB));
  background:-moz-linear-gradient(top, #888, #BBB);
  width:320px;
  margin:20px;
  padding:10px 10px;
  border-radius:20px;
  -webkit-box-shadow:inset #222 0 1px 4px;
}

/* thibaud */
#search_form {
  float:right;
}
#table_spinner{
  position:absolute;
  left:50%;
}
#timeline {
  margin-bottom:15px;
}
select#user_early_access {
  width: 400px;
}

/* remy */
.expandable {
  display:none;
}

.expandable.expanded {
  display:block;
}

h3.expanding_handler a {
  display:block;
  padding:10px 40px;
  background:url(<%= asset_path "sites/expandable_box_arrow@2x.png" %>) no-repeat 23px 50%;
  background-size:10px 13px;
}

h3.expanding_handler.expanded a {
  background:url(<%= asset_path "sites/expandable_box_arrow_active@2x.png" %>) no-repeat 22px 50%;
  background-size:12px 11px;
}

#main form.date_chooser {
  position:absolute;
  top:0;
  right:10px;
}

#main table.crado td {
  padding:5px 5px;
  text-align:left;
}

#main table.crado.center td {
  text-align:center;
}

table#countries_table {
  width:30%;
  float:left;
  margin-right:20px;
}

table#countries_table th {
  background:transparent;
}

table#countries_table td {
 background:transparent;
 border:1px solid #999;
 padding:5px;
 margin:0 0;
}

#chart {
  width:100%;
  height:650px;
}

#series_selectors table {
  border-collapse:collapse;
  background:#333;
  @include border-radius(8px)
}

#series_selectors table td {
  min-width:180px;
  padding:0;
  vertical-align:top;
  text-align:left;
  text-shadow:rgba(0,0,0,0.6) 0 -1px 0;
  border-left:1px solid #222;
  border-right:1px solid #222;
  @include box-shadow(rgba(255,255,255,0.1) 2px 0 0);
}

#series_selectors table td:first-child {
  border-left:none;
}

#series_selectors table td:last-child {
  border-right:none;
  @include box-shadow(none);
}

#series_selectors table td h2 {
  font-size:16px;
  font-weight:400;
  text-transform:uppercase;
  color:#fff;
  letter-spacing:1px;
  padding:10px 10px 8px;
  border-bottom:1px solid #222;
  @include box-shadow(rgba(255,255,255,0.1) 1px 1px 0);
}

#series_selectors table td:last-child h2 {
  border-right:none;
  @include box-shadow(rgba(255,255,255,0.1) 0 1px 0);
}

#series_selectors ul {
  list-style-type:none;
}

#series_selectors li a, #series_selectors li h3 {
  display:block;
  color:#888;
  font-size:16px;
  font-weight:600;
  padding:10px 10px 10px 30px;
  background:rgba(15,15,15,0.3) url(<%= asset_path "admin/selectors_indicator_off.png" %>) no-repeat 8px 50%;
  border-bottom:1px solid #222;
/*  border-left:1px solid #222;
  border-right:1px solid #222;*/
  @include box-shadow(rgba(255,255,255,0.1) 0 1px 0);
}

#series_selectors li li a {
  font-weight:normal;
  background-color:rgba(140,140,140,0.3);
  color:#999;
  font-size:13px;
  padding-top:5px;
  padding-bottom:5px;
/*  padding-left:45px;*/
/*  background-position:23px 50%;*/
}

#series_selectors li li li a {
  background-color:rgba(86,86,86,0.3);
  font-size:12px;
  padding-left:45px;
  background-position:23px 50%;
}

#series_selectors li li li li a {
  padding-left:65px;
  background-position:43px 50%;
}

/*#series_selectors li a.title, #series_selectors li h3 {
  background-color:rgba(255,255,255,0.08);
  color:#999;
}*/

#series_selectors li a.active {
  color:#fff;
  background-image:url(<%= asset_path "admin/selectors_indicator_on.png" %>);
}

#series_selectors li h3 {
  font-weight:normal;
  background-image:none;
  padding-left:10px;
}

.ui-resizable-helper { border: 2px dotted #00F; }

.stats #dates_range {
  position:absolute;
  right:20px;
  top:0;
}

.stats #time_range_title {
  float:left;
  margin:25px 0;
  font-weight:normal;
  min-height:25px;
  min-width:310px;
  position:relative;
  padding:12px 25px 10px 15px;
  border: 1px solid #ccc;
  cursor:pointer;
  @include border-radius(8px)
}

.stats #time_range_title:active {
  @include gradient(rgba(255,255,255,0), rgba(255,255,255,0.25), transparent);
}

.stats #time_range_title .arrow {
  display:block;
  width:9px;
  height:7px;
  background:url(<%= asset_path "select_arrow.png" %>) no-repeat;
  position:absolute;
  right:13px;
  top:48%;
}

.stats #time_range_title .left, .stats #time_range_title .right {
  display:block;
  float:left;
  padding:0 10px 0 0;
}

.stats #time_range_title .middle {
  display:block;
  float:left;
  padding:0 10px 0 0;
}

.stats #time_range_title .day {
  font-size:24px;
  font-weight:600;
  display:block;
}

.stats #time_range_title .time {
  display:block;
  font-size:16px;
  color:#465b7d;
  font-weight:normal;
  padding:0 0 0 16px;
  background:url(<%= asset_path "stats/clock_icon.png" %>) no-repeat 0 4px;
}

.stats #date_pickers {
  display:none;
  position:absolute;
  top:25px;
  right:0;
  z-index:20;
  width:600px;
/*  height:350px;*/
  border: 1px solid #395176;
  @include gradient(#c8d6eb, #b4c7e3, #bfcfe7);
  @include border-radius(5px);
  @include box-shadow((inset #fff 0 1px 0, rgba(0,36,92,0.75) 0 1px 10px));
}

.stats #date_pickers .box {
  width:280px;
  float:left;
  padding:0 0 0 14px;
}

.stats #date_pickers h4 {
  font-size:17px;
  font-weight:600;
  padding:15px 0 10px;
  text-shadow:#fff 0 1px 0;
}

.stats #date_pickers .picker {
  width:279px;
  padding-bottom:6px;
  @include gradient(#e0e8f3, #c5d4e9, #d5dfef);
  @include border-radius(5px);
  @include box-shadow((inset #fff 0 1px 0, rgba(0,41,105,0.72) 0 1px 3px));
}

.stats #date_pickers .picker * {
  padding:0;
}

.stats #date_pickers .ui-datepicker-header {
  padding:15px 0 6px;
}

.stats #date_pickers .ui-datepicker-prev, .stats #date_pickers .ui-datepicker-next {
  display:block;
  width:16px;
  height:16px;
  margin:0 16px;
  float:left;
  margin-top:2px;
  cursor:pointer;
  background:url(<%= asset_path "stats/date_picker_arrow_left.png" %>) no-repeat;
}

.stats #date_pickers .ui-datepicker-next {
  float:right;
  background:url(<%= asset_path "stats/date_picker_arrow_right.png" %>) no-repeat;
}

.stats #date_pickers .ui-datepicker-header .ui-state-disabled {
  cursor:default;
  opacity: 0.3;
  filter: alpha(opacity=30);
}

.stats #date_pickers .ui-datepicker-prev span, .stats #date_pickers .ui-datepicker-next span {display:none}

.stats #date_pickers .ui-datepicker-title {
  width:176px;
  margin:0 auto;
}

.stats #date_pickers .ui-datepicker-title select {
  width:80px;
  height:21px;
  padding-left:5px;
  border:1px solid #9aaac4;
  font-family:inherit;
  font-size:14px;
  color:#4a648e;
  text-shadow:#fff 0 1px 0;
  background:url(<%= asset_path "stats/date_picker_select_back.png" %>) no-repeat 100% 0;
  @include border-radius(4px);
  @include box-shadow(rgba(255,255,255,0.4) 0 1px 0);
  -webkit-appearance:none;
}

.stats #date_pickers .ui-datepicker-month {
  margin-right:10px;
}

.stats #date_pickers .ui-datepicker-calendar {
  border-collapse:collapse;
  width:266px;
  text-align:center;
  margin:0 6px;
}

.stats #date_pickers .ui-datepicker-calendar th {
  font-size:13px;
  color:#4a648e;
  background:none;
}

.stats #date_pickers .ui-datepicker-calendar td {
  width:36px;
  height:36px;
  font-size:18px;
  font-weight:600;
  color:#1e3966;
  border: 1px solid #acbad1;
  @include gradient(#dbe4f4,#c1cfe3,#cdd9ec);
  @include box-shadow(inset rgba(255,255,255,0.2) 1px 1px 0)
}

.stats #date_pickers .ui-datepicker-calendar td a, .stats #date_pickers .ui-datepicker-calendar td span {
  color:#1e3966;
  display:block;
  width:37px;
  height:36px;
  line-height:38px;
}

.stats #date_pickers .ui-datepicker-calendar td a.ui-state-active {
  color:#f1f6ff;
  text-shadow:rgba(0,0,0,0.5) 0 -1px 0;
  @include gradient(#1d52aa,#3a7cd3,#2a67bf);
  @include box-shadow(inset rgba(0,0,0,1) 0 1px 9px);
}

.stats #date_pickers .ui-datepicker-calendar td.ui-state-disabled span {
  color:#a7b7d0;
}

.stats #date_pickers .actions {
  float:right;
  padding:14px;
}

.stats #date_pickers button.cancel {
  background:none;
  border:none;
  outline:0;
  font-family:inherit;
  color:#4a648e;
  font-weight:600;
  font-size:14px;
  display:inline;
  cursor:pointer;
  border-bottom:1px solid #9aafce;
  text-shadow:rgba(255,255,255,0.5) 0 1px 0;
}

.stats #date_pickers button.apply {
  margin-left:10px;
  font-family:inherit;
  font-size:18px;
  font-weight:600;
  color:#f1f6ff;
  border:1px solid #263c5e;
  text-shadow:rgba(0,0,0,0.5) 0 -1px 0;
  padding:6px 12px;
  cursor:pointer;
  @include gradient(#8b9cb8,#4a5e7e,#637898);
  @include border-radius(5px);
  @include box-shadow((inset rgba(255,255,255,0.22) 0 1px 0, #001d3d 0 1px 4px));
}

.stats #date_pickers button.apply:active {
  @include gradient(#4a5e7e,#8b9cb8,#637898);
}

.stats #period_selectors {
  float:right;
  padding:30px 5px 10px;
  list-style-type:none;
}

.stats #period_selectors li {
  float:left;
  margin-left:10px;
}

.stats #period_selectors li a {
  display:inline-block;
  padding:5px 7px;
  color:#fff;
  background:#111;
  @include border-radius(5px);
}

.stats #period_selectors li a:active {
  background:#555;
}

#invoice_popup {
  color:#fff;
  background:rgba(0,0,0,0.85);
  border: 1px solid #000;
  @include border-radius(6px);
  @include box-shadow((#000 0 2px 10px, inset rgba(255,255,255,0.2) 0 1px 0));
}

#invoice_popup ul {
  list-style-type:none;
}

#invoice_popup li {
  padding:8px 0;
  border-bottom: 1px solid #000;
  @include box-shadow(rgba(255,255,255,0.14) 0 1px 0);
}

#invoice_popup li:last-child {
  border-bottom:none;
  @include box-shadow(none);
}

.filters a.active {
  padding:2px;
  color:#fff;
  text-shadow:#000 0 -1px 0;
  background:#444;
  background:-webkit-gradient(linear,left top,left bottom,from(#555),to(#333));
  background:-moz-linear-gradient(top, #555, #333);
  border-radius:5px;
}

.ui-icon { width: 16px; height: 16px; background-image: url(<%= asset_path "admin/ui-icons_222222_256x240.png" %>); }
/* states and images */
.ui-icon { width: 16px; height: 16px; background-image: url(<%= asset_path "admin/ui-icons_222222_256x240.png" %>); }
.ui-widget-content .ui-icon {background-image: url(<%= asset_path "admin/ui-icons_222222_256x240.png" %>); }
.ui-widget-header .ui-icon {background-image: url(<%= asset_path "admin/ui-icons_222222_256x240.png" %>); }
.ui-state-default .ui-icon { background-image: url(<%= asset_path "admin/ui-icons_222222_256x240.png" %>); }
.ui-state-hover .ui-icon, .ui-state-focus .ui-icon {background-image: url(<%= asset_path "admin/ui-icons_222222_256x240.png" %>); }
.ui-state-active .ui-icon {background-image: url(<%= asset_path "admin/ui-icons_222222_256x240.png" %>); }
.ui-state-highlight .ui-icon {background-image: url(<%= asset_path "admin/ui-icons_222222_256x240.png" %>); }
.ui-state-error .ui-icon, .ui-state-error-text .ui-icon {background-image: url(<%= asset_path "admin/ui-icons_222222_256x240.png" %>); }

/* positioning */
.ui-icon-carat-1-n { background-position: 0 0; }
.ui-icon-carat-1-ne { background-position: -16px 0; }
.ui-icon-carat-1-e { background-position: -32px 0; }
.ui-icon-carat-1-se { background-position: -48px 0; }
.ui-icon-carat-1-s { background-position: -64px 0; }
.ui-icon-carat-1-sw { background-position: -80px 0; }
.ui-icon-carat-1-w { background-position: -96px 0; }
.ui-icon-carat-1-nw { background-position: -112px 0; }
.ui-icon-carat-2-n-s { background-position: -128px 0; }
.ui-icon-carat-2-e-w { background-position: -144px 0; }
.ui-icon-triangle-1-n { background-position: 0 -16px; }
.ui-icon-triangle-1-ne { background-position: -16px -16px; }
.ui-icon-triangle-1-e { background-position: -32px -16px; }
.ui-icon-triangle-1-se { background-position: -48px -16px; }
.ui-icon-triangle-1-s { background-position: -64px -16px; }
