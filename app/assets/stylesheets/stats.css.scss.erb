.stats .expander {
  display:block;
  text-align:center;
}

.stats .expander a {
  position:relative;
  display:inline-block;
  padding:6px 24px 6px 10px;
  border:1px solid #3a5175;
  border-top:none;
  color:#f1f6ff;
  font-size:16px;
  font-weight:500;
  text-shadow:rgba(0,0,0,0.6) 0 -1px 0;
  @include gradient(rgba(35,65,106,0.4),rgba(22,51,92,0.5),#627796);
  @include border-radius(0 0 5px 5px);
  @include box-shadow(rgba(0,28,85,0.6) 0 1px 4px);
}

.stats .expander a .arrow {
  display:block;
  position:absolute;
  right:7px;
