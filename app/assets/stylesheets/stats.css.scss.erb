.stats #top_videos td.video .wrap {
  position:relative;
  padding:10px 12px;
}

.stats #top_videos td.video .crop {
  display:inline-block;
  width:96px;
  height:54px;
  position:relative;
  border:1px solid #000f24;
  line-height:0;
  float:left;
  overflow:hidden;
  background:#000;
  @include border-radius(5px);
  @include box-shadow(#002564 0 1px 4px);
}

