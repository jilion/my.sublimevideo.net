.stats #top_videos td.video .right {
  float:left;
  padding:8px 0 0 14px;
}

.stats #top_videos td.video h4 {
  font-size:18px;
  font-weight:600;
  padding-bottom:2px;
  float:left;
}

.stats #top_videos td.video .video_uid {
  display:inline-block;
  padding:2px 5px;
  background:#778cac;
  background:rgba(74,100,142,0.5);
  color:#b7c5db;
  font-size:13px;
  font-weight:600;
  text-shadow:none;
  @include border-radius(4px);
  @include box-shadow(rgba(255,255,255,0.2) 0 1px 0);
}

.stats #top_videos td.video a.provider {
  display:inline-block;
}

.stats #top_videos td.video a.provider.youtube {
  width:45px;
  height:20px;
  background:url(<%= asset_path "stats/youtube_label@2x.png" %>) no-repeat;
  background-size:100%;
  margin:3px 0 -6px 5px;
}

.stats #top_videos td.video a.provider.vimeo {
  width:56px;
  height:17px;
  background:url(<%= asset_path "stats/vimeo_label@2x.png" %>) no-repeat;
  background-size:100%;
  margin:3px 0 -3px 4px;
