  @include border-radius(5px);
}

#last_plays {
  float:right;
  width:59%;
}

#last_plays h4.title {
  padding-bottom:8px;
  font-size:16px;
  font-weight:600;
}

.stats .graph_box {
  float:left;
  width:260px;
  margin-bottom:40px;
}

.stats #last_plays .graph_box {
  margin-bottom:0;
}

.stats #browsers_and_platforms.graph_box, .stats #countries.graph_box {
  margin-right:40px;
}

.stats .graph_box h3.title {
  text-align:center;
  padding-bottom:5px;
}

.stats .graph_box h5.sub_title {
  text-align:center;
  font-size:16px;
  font-weight:normal;
  color:#465b7d;
}

.stats .graph_box .content {
  margin-top:20px;
  min-height:100px;
