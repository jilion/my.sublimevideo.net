@import "css3";

span.beta_badge {
  display:block;
  width:60px;
  height:28px;
  background:url(<%= asset_path "stats/beta_badge.png" %>) no-repeat;
  position:absolute;
  top:58px;
  right:44px;
}

#trial .wrap {
  width:960px;
  height:42px;
  position:relative;
  top:-17px;
  left:-41px;
  border: 1px solid #000;
  text-align:center;
  font-size:17px;
  text-shadow:rgba(0,0,0,0.5) 0 -1px 0;
  @include gradient(#2f5790,#1d3659,#264675);
  @include border-radius(4px 4px 0 0);
  @include box-shadow((inset #4783c8 0 1px 0, inset rgba(81,152,255,0.58) 0 0 20px, inset rgba(81,152,255,0.58) 0 0 20px));
}

#trial .wrap.during {
  @include gradient(#d34a0f,#b72e09,#c53c0c);
  @include box-shadow((inset #fec72c 0 1px 0, inset #ff7d1d 0 0 20px, inset #ff7d1d 0 0 20px));
}

#trial .before {
  color:#bfd8ff;
}

#trial .during {
  color:#ffe6dd;
}

#trial .before p {
  padding:8px 0 0;
}

#trial .during p {
  padding:11px 0 0;
}

#trial p strong, #trial a {
  color:#fff;
  font-weight:600;
}

#trial a {
  font-weight:bold;
  border-bottom:1px solid rgba(255,255,255,0.3);
}

#trial button {
  font-family:inherit;
  color:#fff;
  font-size:15px;
  font-weight:600;
  padding:4px 12px;
/*  margin-left:10px;*/
  cursor:pointer;
  text-shadow:rgba(0,0,0,0.5) 0 1px 2px;
  border: 1px solid #14325f;
  @include gradient(#f69619,#bc1704,#da580f);
  @include border-radius(20px);
  @include box-shadow((inset #fbcb26 0 1px 0, rgba(255,255,255,0.3) 0 1px 0));
}

#trial button:active {
  @include gradient(#bc1704,#f69619,#da580f);
  @include box-shadow(rgba(255,255,255,0.3) 0 1px 0);
}

.stats {
  padding-bottom:40px;
}

.stats #sticky_notice {
  width: 872px;
  margin: 0px 0 20px 0;
}

.stats #period_selectors {
  list-style-type:none;
  margin-top:10px;
}

.stats #period_selectors li {
  width:138px;
  height:82px;
  padding:8px 10px;
  margin-right:20px;
  float:left;
  border: 1px solid #192d4c;
  text-align:center;
  color:#d5e5ff;
  text-shadow:#000 0 -1px 0;
  @include gradient(rgba(56,71,100,0.8),rgba(29,46,79,0.8),#405371);
  @include border-radius(5px);
  @include box-shadow((inset #6981a6 0 1px 0, #283953 0 1px 5px));
}

.stats #period_selectors li.selected {
  border-color:#0a1b37;
  @include gradient(rgba(16,53,118,0.8),rgba(28,76,171,0.8),#164292);
  @include box-shadow((inset rgba(0,0,0,0.8) 0 1px 18px, rgba(255,255,255,0.2) 0 1px 0));
}

.stats #period_selectors li:last-child {
  margin-right:0;
}

.stats #period_selectors li * {cursor:pointer}

.stats #period_selectors li span.title {
  display:block;
  font-size:12px;
  text-transform:uppercase;
  letter-spacing:1px;
  padding:0 0 4px;
}

.stats #period_selectors li .spin {
  width:100%;
  height:62px;
}

.stats #period_selectors li .sparkline {
  height:42px;
}

.stats #period_selectors li span.vv_total {
  display:block;
  font-size:18px;
  font-weight:500;
  padding:5px 0 0;
}

.stats #period_selectors li .message {
  padding:10px 0 0;
}

.stats #period_selectors li .message a {
  color:#fff;
}

.stats #period_selectors li .message a strong {
  font-weight:600;
}

.stats #period_selectors li .message a em {
  font-style:normal;
  font-size:14px;
  color:#d5e5ff;
}

.stats p.days_period_note {
  font-size:11px;
  padding:0;
  position:relative;
  top:10px;
}

.stats #dates_range {
  position:relative;
  float:left;
}

.stats #time_range_title {
  float:left;
  margin:25px 0 0;
  font-weight:normal;
  min-height:45px;
  min-width:310px;
}

.stats #time_range_title.editable {
  position:relative;
  padding:12px 25px 10px 15px;
  border: 1px solid #5e75a2;
  cursor:pointer;
  @include gradient(rgba(255,255,255,0.25), rgba(255,255,255,0), #98acc6);
  @include border-radius(4px);
  @include box-shadow((inset rgba(255,255,255,0.6) 0 1px 0, rgba(255,255,255,0.2) 0 1px 0))
}

.stats #time_range_title.editable:active {
  @include gradient(rgba(255,255,255,0), rgba(255,255,255,0.25), transparent);
}

.stats #time_range_title.editable .arrow {
  display:block;
  width:9px;
  height:7px;
  background:url(<%= asset_path "select_arrow.png" %>) no-repeat;
  position:absolute;
  right:13px;
  top:48%;
}

.stats #time_range_title .left, .stats #time_range_title .right {
  display:block;
  float:left;
  padding:0 10px 0 0;
}

.stats #time_range_title .middle {
  display:block;
  float:left;
  padding:0 10px 0 0;
}

.stats #time_range_title .day {
  font-size:24px;
  font-weight:600;
  display:block;
}

.stats #time_range_title .time {
  display:block;
  font-size:16px;
  color:#465b7d;
  font-weight:normal;
  padding:0 0 0 16px;
  background:url(<%= asset_path "stats/clock_icon.png" %>) no-repeat 0 4px;
}

.stats #date_pickers {
  display:none;
  position:absolute;
  top:25px;
  left:0;
  z-index:20;
  width:600px;
/*  height:350px;*/
  border: 1px solid #395176;
  @include gradient(#c8d6eb, #b4c7e3, #bfcfe7);
  @include border-radius(5px);
  @include box-shadow((inset #fff 0 1px 0, rgba(0,36,92,0.75) 0 1px 10px));
}

.stats #date_pickers .box {
  width:280px;
  float:left;
  padding:0 0 0 14px;
}

.stats #date_pickers h4 {
  font-size:17px;
  font-weight:600;
  padding:15px 0 10px;
  text-shadow:#fff 0 1px 0;
}

.stats #date_pickers .picker {
  width:279px;
  padding-bottom:6px;
  @include gradient(#e0e8f3, #c5d4e9, #d5dfef);
  @include border-radius(5px);
  @include box-shadow((inset #fff 0 1px 0, rgba(0,41,105,0.72) 0 1px 3px));
}

.stats #date_pickers .ui-datepicker-header {
  padding:15px 0 6px;
}

.stats #date_pickers .ui-datepicker-prev, .stats #date_pickers .ui-datepicker-next {
  display:block;
  width:16px;
  height:16px;
  margin:0 16px;
  float:left;
  margin-top:2px;
  cursor:pointer;
  background:url(<%= asset_path "stats/date_picker_arrow_left.png" %>) no-repeat;
}

.stats #date_pickers .ui-datepicker-next {
  float:right;
  background:url(<%= asset_path "stats/date_picker_arrow_right.png" %>) no-repeat;
}

.stats #date_pickers .ui-datepicker-header .ui-state-disabled {
  cursor:default;
  opacity: 0.3;
  filter: alpha(opacity=30);
}

.stats #date_pickers .ui-datepicker-prev span, .stats #date_pickers .ui-datepicker-next span {display:none}

.stats #date_pickers .ui-datepicker-title {
  width:176px;
  margin:0 auto;
}

.stats #date_pickers .ui-datepicker-title select {
  width:80px;
  height:21px;
  padding-left:5px;
  border:1px solid #9aaac4;
  font-family:inherit;
  font-size:14px;
  color:#4a648e;
  text-shadow:#fff 0 1px 0;
  background:url(<%= asset_path "stats/date_picker_select_back.png" %>) no-repeat 100% 0;
  @include border-radius(4px);
  @include box-shadow(rgba(255,255,255,0.4) 0 1px 0);
  -webkit-appearance:none;
}

.stats #date_pickers .ui-datepicker-month {
  margin-right:10px;
}

.stats #date_pickers .ui-datepicker-calendar {
  border-collapse:collapse;
  width:266px;
  text-align:center;
  margin:0 6px;
}

.stats #date_pickers .ui-datepicker-calendar th {
  font-size:13px;
  color:#4a648e;
}

.stats #date_pickers .ui-datepicker-calendar td {
  width:36px;
  height:36px;
  font-size:18px;
  font-weight:600;
  color:#1e3966;
  border: 1px solid #acbad1;
  @include gradient(#dbe4f4,#c1cfe3,#cdd9ec);
  @include box-shadow(inset rgba(255,255,255,0.2) 1px 1px 0)
}

.stats #date_pickers .ui-datepicker-calendar td a, .stats #date_pickers .ui-datepicker-calendar td span {
  color:#1e3966;
  display:block;
  width:37px;
  height:36px;
  line-height:38px;
}

.stats #date_pickers .ui-datepicker-calendar td a.ui-state-active {
  color:#f1f6ff;
  text-shadow:rgba(0,0,0,0.5) 0 -1px 0;
  @include gradient(#1d52aa,#3a7cd3,#2a67bf);
  @include box-shadow(inset rgba(0,0,0,1) 0 1px 9px);
}

.stats #date_pickers .ui-datepicker-calendar td.ui-state-disabled span {
  color:#a7b7d0;
}

.stats #date_pickers .actions {
  float:right;
  padding:14px;
}

.stats #date_pickers button.cancel {
  background:none;
  border:none;
  outline:0;
  font-family:inherit;
  color:#4a648e;
  font-weight:600;
  font-size:14px;
  display:inline;
  cursor:pointer;
  border-bottom:1px solid #9aafce;
  text-shadow:rgba(255,255,255,0.5) 0 1px 0;
}

.stats #date_pickers button.apply {
  margin-left:10px;
  font-family:inherit;
  font-size:18px;
  font-weight:600;
  color:#f1f6ff;
  border:1px solid #263c5e;
  text-shadow:rgba(0,0,0,0.5) 0 -1px 0;
  padding:6px 12px;
  cursor:pointer;
  @include gradient(#8b9cb8,#4a5e7e,#637898);
  @include border-radius(5px);
  @include box-shadow((inset rgba(255,255,255,0.22) 0 1px 0, #001d3d 0 1px 4px));
}

.stats #date_pickers button.apply:active {
  @include gradient(#4a5e7e,#8b9cb8,#637898);
}

.stats #export {
  float:left;
  margin:10px 20px 10px 15px;
}

.stats #export button, .stats #export a {
  display:inline-block;
  border: 1px solid #5e75a2;
  font-family:inherit;
  font-size:14px;
  font-weight:600;
  color:#213866;
  padding:3px 7px;
  cursor:pointer;
  text-shadow:rgba(255,255,255,0.3) 0 1px 0;
  @include gradient(rgba(255,255,255,0.25), rgba(255,255,255,0), #98acc6);
  @include border-radius(4px);
  @include box-shadow((inset rgba(255,255,255,0.6) 0 1px 0, rgba(255,255,255,0.2) 0 1px 0))
}

.stats #export button:active, .stats #export a:active {
  @include gradient(rgba(255,255,255,0), rgba(255,255,255,0.25), transparent);
}

.stats #export button:disabled, .stats #export a:disabled {
  cursor:default;
  opacity:0.5;
}

.stats #export button span.icon, .stats #export a span.icon {
  padding:2px 0 2px 18px;
  background:url(<%= asset_path "stats/export_icon.png" %>) no-repeat 0 50%;
}

#content #export.tooltip .content.hidden {display:none}

#content #export.tooltip .content {
  bottom:38px;
  left:-13px
}

#content #export_wrap p.desc {
  font-size:14px;
  color:#465B7D;
  line-height:20px;
  margin:4px 0 0 15px;
  padding-right:100px;
}

.stats #plays_and_loads_graph_legend {
  float:right;
  list-style-type:none;
  padding-top:70px;
  margin-bottom:25px;
}

.stats.free #plays_and_loads_graph_legend {padding-top:45px}

.stats #plays_and_loads_graph_legend li {
  float:left;
  font-size:22px;
  padding:0 0 0 35px;
  margin:0 10px;
}

.stats #plays_and_loads_graph_legend li.video_plays {
  background:url(<%= asset_path "stats/video_plays_label.png" %>) no-repeat;
}

.stats #plays_and_loads_graph_legend li.page_visits {
  background:url(<%= asset_path "stats/page_visits_label.png" %>) no-repeat;
}

.stats #plays_and_loads_graph_legend li span {
  font-size:16px;
}

.stats #plays_and_loads {
  min-height:100px;
  padding:16px 16px 4px;
  background:#6a86a8;
  background:rgba(0,36,91,0.25);
  @include border-radius(5px);
  @include box-shadow((rgba(255,255,255,0.3) 0 1px 0, inset rgba(0,27,68,0.5) 0 1px 4px));
}

.stats #plays_and_loads p.note {
  font-size:11px;
  text-align:right;
  position:relative;
  right:-10px;
  padding-bottom:1px;
}

.stats h3.title {
  font-size:34px;
  font-weight:600;
  padding:35px 0 10px;
}

.stats h3.title.first {
  padding-top:0;
}

.stats #top_videos_content {
  min-height:100px;
}

.stats #top_videos table, .stats #top_videos .empty_box {
  width:100%;
  border-collapse:collapse;
  background:#8fa0c1;
  background:rgba(0,36,91,0.1);
  @include border-radius(5px);
  @include box-shadow((rgba(255,255,255,0.3) 0 1px 0, inset rgba(0,27,68,0.6) 0 1px 4px));
}

.stats #top_videos .empty_box {
  height:70px;
  line-height:70px;
  text-align:center;
  color:#465b7d;
  font-weight:600;
}

.stats #top_videos table col.alt {
  background:rgba(24,41,78,0.05);
}

.stats #top_videos table th {
  background:#6c81a0;
  background:rgba(4,27,62,0.4);
  color:#aabcd5;
  text-shadow:rgba(0,0,0,0.6) 0 -1px 0;
  padding:8px 10px;
  font-size:15px;
  font-weight:600;
  border-bottom:1px solid rgba(0,0,0,0.25);
}

.stats #top_videos table th.video {
  width:398px;
  text-align:left;
