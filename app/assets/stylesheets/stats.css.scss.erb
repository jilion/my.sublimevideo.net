  height:7px;
  background:url(<%= asset_path "stats/expand_videos_arrow_down.png" %>) no-repeat;
}

.stats .expander a .arrow.up {
  background:url(<%= asset_path "stats/expand_videos_arrow_up.png" %>) no-repeat;
}

#last_60_minutes_plays_and_loads {
  float:left;
  width:39%;
  margin:35px 0 0;
}

#last_60_minutes_loads, #last_60_minutes_plays {
  height:80px;
  padding:8px 0;
}

#last_60_minutes_plays_and_loads .sum {
  float:left;
  width:100px;
  font-weight:600;
  text-align:center;
}

#last_60_minutes_plays_and_loads .sum strong {
  display:block;
  font-size:23px;
  font-weight:600;
}

#last_60_minutes_plays_and_loads .sparkline {
  float:left;
  width:200px;
  height:50px;
  text-align:center;
  color:#d5e5ff;
