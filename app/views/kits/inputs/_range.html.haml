- value = settings[key] || default
.entry.range
  %span{ style: 'font-weight:600' }= label_tag "kit[addons][#{addon.id}][#{key}]", t("kit.#{addon.name}.settings.#{key}")
  %br
  = range_field_tag "kit[addons][#{addon.id}][#{key}]", value, in: in_range, step: step, style: 'display:inline'
  %span{ id: "kit_addons_#{addon.id}_#{key}_value" }= value
.spacer
