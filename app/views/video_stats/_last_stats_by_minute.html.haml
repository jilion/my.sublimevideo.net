%h3.title#top_videos_title{style: "padding-top:10px"} Last 60 minutes

#last_60_minutes_loads_and_plays
  = render 'last_60_minutes_loads_and_plays', stats_presenter: stats_presenter

#last_plays.graph_box
  %h4.title Last plays
  .content_wrap{ style: 'overflow:hidden' }
    - if stats_presenter.last_plays.empty?
      %p.empty{style: "text-align:center"} No plays.
    - else
      %ul= render partial: 'last_play', collection: stats_presenter.last_plays
.spacer
