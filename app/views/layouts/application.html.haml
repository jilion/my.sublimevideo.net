- @ie_min = 8
- @page_title_prefix ||= "MySublimeVideo"
- @body_class = @body_class.to_s + " #{early_access_body_class}"

= content_for :javascript do
  -# if %w[site_stats video_codes].exclude?(controller_name)
  = javascript_include_tag 'https://d3dy5gmtp8yhk7.cloudfront.net/1.11/pusher.min.js'
  = javascript_include_tag 'application'
  = sublimevideo_include_tag(request.ssl?, :my).html_safe

- if flash.present?
  = content_for :middle do
    = render "layouts/flash", flash: flash

- if Rails.env.production? && user_signed_in?
  = content_for :javascript_bottom do
    = render 'layouts/fastpass'

= render 'layouts/menu'

= render template: 'layouts/sublimevideo'
