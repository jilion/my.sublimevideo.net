$('users_table_wrap').update("<%= escape_javascript(render 'users') %>");
$('table_spinner').hide();
<% if params[:search] %>
  <% if params[:search].blank? %>
    $('users_title').update("<%= pluralize(@users.total_entries, 'User') %>");
  <% else %>
    $('users_title').update("<%= "#{pluralize(@users.total_entries, 'User')} that contains '#{params[:search]}'" %>");
  <% end %>
<% end %>
$(document.body).scrollTo();
$('search_button').disabled = false;
