= title_and_content_header("Users")

= link_to "Invite a user", new_user_invitation_path, :class => "add"

%table.main#users
  %tr
    %th= "Full name"
    %th= "Email"
    %th= "Trial?"
    %th= "# Sites"
    %th= "# Videos"
    %th= "CC?"
    %th.delete= "Confirmed?"
  = render @users
  
= will_paginate @users