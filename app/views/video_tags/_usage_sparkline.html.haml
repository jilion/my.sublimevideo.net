- starts = last_grouped_starts(video_tag.starts, last_starts_days)

.sparkline{ id: "usage_sparkline_#{video_tag.uid}" }
  :coffee
    $('#usage_sparkline_#{video_tag.uid}').sparkline #{starts},
      width:    '100%'
      height:   '28px'
      lineColor: '#00ff18'
      disableHighlight: true
      disableTooltips: true
      spotRadius: 0
      fillColor: 'rgba(79,249,99,.4)'
