# MP4 720p
Command: HandBrakeCLI -i $input_file$ -o $output_file$ -e x264 -q 0.589999973773956 -a 1,1 -E faac,ac3 -B 192,auto -R 48,Auto -6 dpl2,auto -f mp4 -X 1280 -Y 1280 -O -P -m -x level=30:cabac=0:ref=3:mixed-refs=1:analyse=all:me=umh:no-fast-pskip=1

# MP4 480p
Command: HandBrakeCLI -i $input_file$ -o $output_file$ -e x264 -q 0.589999973773956 -a 1,1 -E faac,ac3 -B 160,auto -R 48,Auto -6 dpl2,auto -f mp4 -X 640 -Y 360 -O -P -m -x level=30:cabac=0:ref=3:mixed-refs=1:analyse=all:me=umh:no-fast-pskip=1
